{
  "$schema": "http://json-schema.org/schema",
  "id": "custom-react",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
    "x-prompt": "What name would you like to use for the component?"
    }
  },
  "required": ["name"]
}
